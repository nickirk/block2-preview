
@PACKAGE_INIT@

SET(PN block2)
SET(${PN}_VERSION 2.0)

SET_AND_CHECK(${PN}_INCLUDE_DIR "@PACKAGE_CMAKE_INSTALL_INCLUDEDIR@")
SET_AND_CHECK(${PN}_INCLUDE_DIRS "@PACKAGE_CMAKE_INSTALL_INCLUDEDIR@")
SET_AND_CHECK(${PN}_LIBRARY "@PACKAGE_CMAKE_INSTALL_LIBDIR@")
SET_AND_CHECK(${PN}_LIBRARIES "@PACKAGE_CMAKE_INSTALL_LIBDIR@")
SET(${PN}_FOUND 1)

CHECK_REQUIRED_COMPONENTS(${PN})

IF (NOT TARGET ${PN}::${PN})
    INCLUDE("${CMAKE_CURRENT_LIST_DIR}/${PN}Targets.cmake")
ENDIF()
